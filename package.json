{
  "name": "ng-universal-demo",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "scripts": {
    "start": "npm run build && npm run server",
    "build": "webpack",
    "build:aot": "webpack --env.aot --env.client & webpack --env.aot --env.server",
    "build:prod": "webpack --env.aot --env.client --define process.env.NODE_ENV='production' & webpack --env.aot --env.server",
    "prebuild": "npm run clean",
    "prebuild:aot": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist",
    "server": "nodemon dist/server.js",
    "watch": "webpack --watch"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.3.0",
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@angular/platform-server": "^4.3.0",
    "@angular/router": "^4.3.0",
    "@nguniversal/express-engine": "^1.0.0-beta.2",
    "express": "^4.15.3",
    "rxjs": "^5.4.2",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.13"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.3.0",
    "@ngtools/webpack": "^1.5.1",
    "@types/express": "^4.0.36",
    "@types/node": "^8.0.14",
    "html-webpack-plugin": "^2.29.0",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "typescript": "^2.4.1",
    "webpack": "^3.3.0",
    "webpack-merge": "^4.1.0"
  }
}
